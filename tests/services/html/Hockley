    <script>
        function getParameterByName(name, url) {
          if (!url) url = window.location.href;
          name = name.replace(/[\[\]]/g, "\$&");
          var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
              results = regex.exec(url);
          if (!results) return null;
          if (!results[2]) return '';
          return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
      
        function HelpWindow(obj, width, height) {
          
          jQuery('#formInfoModal .modal-body').load(obj.href+' .homeco_help_content', function(html) {
            // set the title
            var pageTitle = jQuery(html).find('h1').html();
            jQuery('#formInfoModal .modal-title').html(pageTitle);
            
            // is there an anchor or help param?
            var hasHash = /#([^ ]*)/;
            var anchor = false;
            if (hasHash.test(obj.href) !== false) {
              anchor = obj.href.split('#')[1];
            } else if (getParameterByName('help', obj.href)) {
              anchor = getParameterByName('help', obj.href);
            }
            
            if (anchor !== false)
            {
              var anchorElement = jQuery('#formInfoModal .modal-body a[name="'+anchor+'"]');
              var anchorContent = anchorElement.closest('div').html();
              jQuery('#formInfoModal .modal-body').html(anchorContent);
            }
              
            // remove h1 from the modal body
            jQuery('#formInfoModal .modal-body').find('h1').remove();

            jQuery('#formInfoModal').modal({
              focus: true,
              show: true
            });
          });
          return false;
        }
    </script>    <!-- autocomplete script -->
